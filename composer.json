{
    "name": "itpalert/web2sms",
    "description": "Web2sms for laravel.",
    "keywords": ["notifications", "web2sms"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gere Attila",
            "email": "gereattila98@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "suggest": {
        "illuminate/support": "Required for Laravel integration (Facade and Service Provider)"
    },
    "autoload": {
        "psr-4": {
            "ITPalert\\Web2sms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ITPalert\\Web2sms\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:unit": "vendor/bin/phpunit --testsuite=Unit",
        "test:integration": "vendor/bin/phpunit --testsuite=Integration"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ITPalert\\Web2sms\\Web2smsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
